material spaceship
{
	technique unlit
	{
		pass 0
		{
			shader
			{
				vertexShader = Shaders3D/3d_position_tex.vert
				fragmentShader = Shaders3D/3d_color_tex.frag
			}
			sampler 0
			{
				path = Sprite3DTest/boss.png
			}
		}
	}

	technique lit
	{
		pass 0
		{
			shader
			{
				defines = MAX_POINT_LIGHT_NUM 1;MAX_SPOT_LIGHT_NUM 1;MAX_DIRECTIONAL_LIGHT_NUM 1
				vertexShader = Shaders3D/3d_position_normal_tex.vert
				fragmentShader = Shaders3D/3d_color_normal_tex.frag
			}
			sampler 0
			{
				path = Sprite3DTest/boss.png
			}
		}
	}

	technique outline
	{
		pass outline thin
		{
			renderState
			{
				cullFace = true
				cullFaceSide = BACK
				depthTest = false
			}
			shader
			{
				vertexShader = Shaders3D/OutLine.vert
				fragmentShader = Shaders3D/OutLine.frag
				// Uniforms
				OutLineColor = 1,1,0
				OutlineWidth = 0.04
			}
			sampler 0
			{
				path = Sprite3DTest/boss.png
			}
		}
		pass outline thick
		{
			renderState
			{
				cullFace = true
				cullFaceSide = FRONT
				depthTest = true
			}
			shader
			{
				vertexShader = Shaders3D/OutLine.vert
				fragmentShader = Shaders3D/OutLine.frag
				// Uniforms
				OutLineColor = 0,0,1
				OutlineWidth = 0.02
			}
			sampler 0
			{
				path = Sprite3DTest/boss.png
			}
		}
		pass 1
		{
			shader
			{
				vertexShader = Shaders3D/3d_position_tex.vert
				fragmentShader = Shaders3D/3d_color_tex.frag
			}
			sampler 0
			{
				path = Sprite3DTest/boss.png
			}
		}
	}
}
